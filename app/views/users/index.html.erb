<div class="col-md-8">
  <h2>List of Users</h2>

  <div class="panel panel-default">
    <div class="panel-body">
      <table class="table table-striped">
        <thead>
          <tr>
             <th>Name</th>
             <th>Email</th>
          </tr>
        </thead>
        <tbody>
           <% @users.each do |user| %>
             <tr>
               <td><%=user.first_name %></td>
               <td><%=user.email %></td>
             </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
